{
    "name": "wiproCompilance",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^7",
        "@sap/cds-hana": "^2",
        "@sap/xsenv": "^5.1.0",
        "@sap/xssec": "^3",
        "express": "^4",
        "passport": "^0.7.0"
    },
    "devDependencies": {
        "@cap-js/sqlite": "^1",
        "@sap/cds-dk": "^7",
        "@sap/eslint-plugin-cds": "^3",
        "@sap/ux-specification": "^1.120.13",
        "eslint": "^9",
        "rimraf": "^5.0.5"
    },
    "scripts": {
        "start": "cds-serve",
        "undeploy": "cf undeploy wiproCompilance --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
        "watch-masterdata": "cds watch --open masterdata/webapp/index.html?sap-ui-xx-viewCache=false",
        "watch-complaince33": "cds watch --open complaince33/webapp/index.html?sap-ui-xx-viewCache=false",
        "watch-masterdata1": "cds watch --open masterdata1/webapp/index.html?sap-ui-xx-viewCache=false"
    },
    "cds": {
        "requires": {
            "auth": "dummy",
            "db": "hana"
        }
    },
    "sapux": [
        "app/masterdata",
        "app/complaince33",
        "app/masterdata1"
    ]
}
